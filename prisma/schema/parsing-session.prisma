model ParsingSession {
  id                Int      @id @unique @default(autoincrement())
  name              String
  status            String
  lastProcessedPage Int
  processedPages    Int
  processedItems    Int
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  @@map("parsing_session")
}

enum ParsingSessionStatus {
  CREATED
  RUNNING
  COMPLETED
  PAUSED
  FAILED
}
